programa {
  funcao inicio() {
    real altura
    real comprimento
    real areaQuadrado

    escreva("Qual a altura do quadrado?: ")
    leia(altura)

    escreva("Qual o comprimento?: ")
    leia(comprimento)

    areaQuadrado = altura * comprimento

    escreva("A area do quadrado é igual a: " + areaQuadrado)
  }
}
